{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "plugins": ["react"],
    "rules": {
        "import/no-extraneous-dependencies": [2, { "devDependencies": true }],
        "jsx-a11y/no-static-element-interactions": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "class-methods-use-this": 0,
        "max-len": ["error", 120, 2]
    }
}